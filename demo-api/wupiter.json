{
  "name": "Spring Boot Accelerator",
  "description": "Code generator for Spring Boot",
  "tags": [ "java", "Spring Boot" ],
  "params": [
    {
      "name": "name",
      "type": "text",
      "description": "Enter your name",
      "defaultValue": "World",
      "required": true
    },
    {
      "name": "checked",
      "type": "checkbox",
      "description": "Check me",
      "defaultValue": true,
      "required": true
    },
    {
      "name": "cool",
      "type": "radios",
      "label": "Are you cool?",
      "defaultValue": "Yes",
      "values": [ "Yes", { "label": "No", "key": "no" } ],
      "required": false
    },
    {
      "name": "birthPlace",
      "type": "searchbox",
      "label": "Where were you born?",
      "required": false,
      "allowMultiple": false,
      "values": [
        { "label": "Canada", "key": "CA" },
        { "label": "Ireland", "key": "IE" },
        { "label": "United States of America", "key": "US" }
      ]
    },
    {
      "name": "cities",
      "type": "searchbox",
      "label": "Favourite cities?",
      "required": false,
      "allowMultiple": true,
      "values": [
        {
          "groupLabel": "Canada",
          "values": [
            { "label": "Toronto" },
            { "label": "Ottawa" },
            { "label": "Montreal" }
          ]
        },
        {
          "groupLabel": "US",
          "values": [
            { "label": "New York" },
            { "label": "Washington DC" },
            { "label": "Philadelphia" }
          ]
        }
      ]
    }
  ],
  "files": [
    {
      "filePath": "hello.html",
      "static": false
    }
  ]
}
